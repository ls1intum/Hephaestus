{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"baseUrl": "./",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"paths": {
			"@/*": ["./src/*"]
		},
		"typeRoots": ["./node_modules/@types"],
		"types": ["node"],
		"strict": true,
		"outDir": "./dist",
		"skipLibCheck": true
	},
	"tsc-alias": {
		"resolveFullPaths": true
	}
}
