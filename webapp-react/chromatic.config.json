{
  "storybookBuildDir": "storybook-static",
  "storybookBaseDir": "webapp-react",
  "storybookConfigDir": "webapp-react/.storybook",
  "exitZeroOnChanges": false,
  "zip": true,
  "junitReport": true,
  "ignoreLastBuildOnBranch": "renovate/**",
  "skip": "dependabot/**",
  "externals": [
    "webapp-react/public/**",
    "webapp-react/src/assets/**",
    "webapp-react/**/*.css",
    "webapp-react/**/*.scss", 
    "webapp-react/**/*.sass",
    "webapp-react/tailwind.config.*",
    "webapp-react/vite.config.*",
    "webapp-react/biome.json",
    "webapp-react/components.json"
  ],
  "onlyChanged": true,
  "traceChanged": "expanded",
  "untraced": [
    "**/*.md",
    "**/*.stories.mdx",
    "webapp-react/.storybook/addons.ts",
    "webapp-react/.storybook/manager.ts",
    "docs/**"
  ]
}
